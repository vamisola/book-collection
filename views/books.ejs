<% include partials/header %>

<h1>My Book collection</h1>
<a href="/books/new">Add new book</a>
<% books.forEach(book => { %>
    <div>
        <h3><%= book.bookTitle %></h3>
        <img src="<%= book.image %>">
        <h4><%= book.author %></h4>
    </div>
<% });%>

<% include partials/footer %>
